{
  "name": "dot-github",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "prettier:check": "prettier --check **/*.{json,md} --ignore-path=.prettierignore",
    "prettier:write": "prettier --write **/*.{json,md} --ignore-path=.prettierignore",
    "lint": "pnpm nx run-many --target=lint --verbose",
    "lint:affected": "pnpm nx affected --target=lint --verbose",
    "build": "pnpm nx run-many --target=build --verbose",
    "build:affected": "pnpm nx affected --target=build --verbose",
    "test": "pnpm nx run-many --target=test --verbose",
    "test:affected": "pnpm nx affected --target=test --verbose",
    "package": "pnpm nx run-many --target=package --verbose",
    "package:affected": "pnpm nx affected --target=package --verbose",
    "ci:changeset:publish": "pnpm changeset publish",
    "ci:changeset:version": "pnpm changeset version && pnpm version --patch"
  },
  "private": true,
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@nx/devkit": "^17.1.3",
    "@nx/esbuild": "^17.1.3",
    "@nx/eslint": "^17.1.3",
    "@nx/eslint-plugin": "^17.1.3",
    "@nx/jest": "^17.1.3",
    "@nx/js": "17.2.8",
    "@nx/plugin": "^17.1.3",
    "@swc-node/register": "~1.8.0",
    "@swc/cli": "~0.1.63",
    "@swc/core": "~1.4.0",
    "@swc/helpers": "~0.5.3",
    "@swc/jest": "0.2.29",
    "@types/jest": "^29.4.0",
    "@types/node": "20.12.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.4.1",
    "jsdom": "~24.0.0",
    "nx": "^17.1.3",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "update-ts-references": "^3.0.0",
    "rimraf": "^5.0.5"
  },
  "repository": "https://github.com/smartcontractkit/.github"
}
