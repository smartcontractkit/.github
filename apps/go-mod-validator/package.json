{
  "name": "go-mod-validator",
  "version": "0.0.0",
  "private": true,
  "description": "Parses 'go.mod' file given as input, and verifies it's dependencies",
  "engines": {
    "pnpm": "9",
    "node": "20"
  },
  "main": "out/index.js",
  "scripts": {
    "local": "GITHUB_STEP_SUMMARY=/dev/null ts-node src/index.ts --local --tokenEnv=GITHUB_TOKEN",
    "help": "ts-node --help",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --tsconfig=./tsconfig.json --outfile=dist/index.js",
    "test": "vitest run"
  },
  "devDependencies": {
    "@octokit/types": "^13.5.0",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.14.9",
    "nock": "^13.5.4",
    "node-fetch": "^2.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@actions/glob": "^0.4.0",
    "minimist": "^1.2.8",
    "octokit": "^3.2.1"
  },
  "author": "@smartcontractkit",
  "license": "MIT",
  "repository": "https://github.com/smartcontractkit/.github"
}
